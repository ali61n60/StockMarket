{
  "compilerOptions": {
    "outDir": "./",
    "noImplicitAny": false,
    "removeComments": false,
    "sourceMap": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "noEmit": false
  },
  "compileOnSave": true,
  "exclude": ["node_modules","wwwroot"],
  "include": [
    "file.ts",
    "*.tsx",
    "*.jsx"]
  
}

