{
  "compilerOptions": {
    "outDir": "./",
    "noImplicitAny": false,
    "removeComments": false,
    "sourceMap": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "noEmit": true

  },
  "compileOnSave": true,
  "exclude": [
    "node_modules",
    "./wwwroot/lib/**/*"
  ]
}